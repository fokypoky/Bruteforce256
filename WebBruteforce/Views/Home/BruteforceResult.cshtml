@model HashOutput;

@{
	Layout = null;
}

<!DOCTYPE HTML>
<html>
<head>
	<title>Bruteforce result</title>
	<link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css"/>
	<link rel="stylesheet" href="/css/styles.css"/>
</head>
<body>

<h5 class="text-black-50 text-center m-2 p-2">BRUTEFORCE RESULT</h5>

<div class="form-group results-block">
	<label>Values</label>
	<textarea class="form-control" rows="5">
		@foreach (string hashResult in Model.Results)
		{
			@hashResult
		}
	</textarea>
</div>

<div class="form-group errors-block">
	<label>Errors</label>
	<textarea class="form-control" rows="3">
		@foreach (string hashError in @Model.Errors)
		{
			@hashError
		}
	</textarea>
</div>

<div class="button-block text-center">
	<a asp-action="Index" class="btn btn-primary">Try again</a>
</div>

</body>
</html>